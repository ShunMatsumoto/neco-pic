.index
  .index__box
    .index__box__header
      .index__box__header__icon
      .index__box__header__menu-list
        %ul.menu-list
          %li.user-name
            = link_to "#", class: "header_list_link" do
              -# %img{src: "icons/猫のシルエット白.png", alt: "white_cat", width: "30px"}
              %p ぬこ様
          %li.new-post
            = link_to "#", class: "header_list_link" do
              %p 新規投稿
          %li.post-index
            = link_to "#", class: "header_list_link" do
              %p 投稿一覧
          %li.logout
            = link_to "#", class: "header_list_link" do
              %p ログアウト


    .index__box__main
      .index__box__main__box
        .index__box__main__box__main-title
          %h2.titile They are wonderful cats
        .index__box__main__box__contents
          .index__box__main__box__contents__content
            %ul.contents
              %li.content
                = link_to "#", class: "cat_list" do
                  = image_tag '/images/cat-1.jpg', alt: 'cat_image', height: '95%', width: '95%', calss: "cat_image"
                  %p neko-pic
              %li.content
                = link_to "#", class: "cat_list" do
                  = image_tag '/images/cat-2.jpg', alt: 'cat_image', height: '95%', width: '95%', calss: "cat_image"
                  %p neko-pic
              %li.content
                = link_to "#", class: "cat_list" do
                  = image_tag '/images/cat-3.jpg', alt: 'cat_image', height: '95%', width: '95%', calss: "cat_image"
                  %p neko-pic
              %li.content
                = link_to "#", class: "cat_list" do
                  = image_tag '/icons/Neco-Pic.png', alt: 'cat_image', height: '95%', width: '95%', calss: "cat_image"
                  %p neko-pic
              %li.content
              %li.content
              %li.content






    .index__box__footer
